{
  "name": "cleanidoc-dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^5.9.0",
    "@sentry/react": "^10.22.0",
    "@sentry/tracing": "^7.120.4",
    "@supabase/supabase-js": "^2.75.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "crypto-browserify": "^3.12.1",
    "date-fns": "^4.1.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.0",
    "jsonwebtoken": "^9.0.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "jszip": "^3.10.1",
    "lucide-react": "^0.546.0",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.10",
    "puppeteer": "^24.27.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^7.9.4",
    "react-scripts": "^5.0.1",
    "speakeasy": "^2.0.0",
    "ssh2-sftp-client": "^12.0.1",
    "uuid": "^13.0.0",
    "web-vitals": "^2.1.4",
    "zod": "^4.1.12"
  },
  "scripts": {
    "start": "craco start",
    "server": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm start\"",
    "build": "craco build",
    "test": "craco test",
    "test:ci": "react-scripts test --watchAll=false --coverage --passWithNoTests",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --max-warnings 50",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "typecheck": "echo 'No TypeScript configured, skipping typecheck'",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:report": "playwright show-report",
    "prepare": "husky"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings 50 --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
